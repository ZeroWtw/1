-- Charload/Misc
local Main = Window:NewTab("CharLoad/Misc")
local MainSection = Main:NewSection("CharLoad")


MainSection:NewButton("(Skip-Char-1)", "(Skips-Your-Saved-First-Character.)", function()
    local args = {
        [1] = "LoadChar1"
    }
    
    game:GetService("Players").LocalPlayer.PlayerGui.MainMenu.RemoteEvent:FireServer(unpack(args))
end)


MainSection:NewButton("(Skip-Char-2)", "(Skips-Your-Saved-Second-Character.)", function()
    local args = {
        [1] = "LoadChar2"
    }
    
    game:GetService("Players").LocalPlayer.PlayerGui.MainMenu.RemoteEvent:FireServer(unpack(args))
end)


MainSection:NewButton("(Skip-Char-1(Shaders)", "(Skips-Your-Saved-First-Character/With-Shaders.)", function()
    repeat wait() until game:IsLoaded() and game:GetService'Players'.LocalPlayer

    if game.PlaceId ~= 1923555882 then return end
    
    local Menu = game:GetService'Players'.LocalPlayer:WaitForChild'PlayerGui':WaitForChild'MainMenu'
    local Remote = Menu:WaitForChild'RemoteEvent'
    
    task.wait(0.35)
    
    if Menu:WaitForChild'Char1'.Value then
        Remote:FireServer'LoadChar1'
    end
    
    workspace.CurrentCamera.CameraType = 'Custom'
    sethiddenproperty(game.Lighting, 'Technology', 'Compatibility')
    game.Lighting:WaitForChild('Blur', 1000000):Destroy()
end)


MainSection:NewButton("(Skip-Char-2(Shaders)", "(Skips-Your-Saved-Second-Character/With-Shaders.)", function()
    repeat wait() until game:IsLoaded() and game:GetService'Players'.LocalPlayer

    if game.PlaceId ~= 1923555882 then return end
    
    local Menu = game:GetService'Players'.LocalPlayer:WaitForChild'PlayerGui':WaitForChild'MainMenu'
    local Remote = Menu:WaitForChild'RemoteEvent'
    
    task.wait(0.35)
    
    if Menu:WaitForChild'Char2'.Value then
        Remote:FireServer'LoadChar2'
    end
    
    workspace.CurrentCamera.CameraType = 'Custom'
    sethiddenproperty(game.Lighting, 'Technology', 'Compatibility')
    game.Lighting:WaitForChild('Blur', 1000000):Destroy()
end)


