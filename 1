local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local colors = {
    SchemeColor = Color3.fromRGB(139,0,0),
    Background = Color3.fromRGB(0, 0, 0),
    Header = Color3.fromRGB(5, 5, 5),
    TextColor = Color3.fromRGB(255,255,255),
    ElementColor = Color3.fromRGB(20, 20, 20)
}
local Window = Library.CreateLib("Skript®(Premium)", colors)
--  Main/Anti
local Main = Window:NewTab("Main/Anti")
local MainSection = Main:NewSection("Main")


MainSection:NewButton("(Bloxr-Run)(RP-Style)", "(This is better for RP will not get caught.)", function()
    local player = game:GetService("Players").LocalPlayer
    local UIS = game:GetService("UserInputService")
    
    function ShiftPressed()
     local ShiftHeld = UIS:IsKeyDown(Enum.KeyCode.LeftShift)
     
     if ShiftHeld then
      player.Character.Humanoid.WalkSpeed = 20
     end
    end
    
    function ShiftReleased()
     local ShiftHeld = UIS:IsKeyDown(Enum.KeyCode.LeftShift)
    
     if ShiftHeld == false then
      player.Character.Humanoid.WalkSpeed = 11.5
     end
    end
    
    UIS.InputBegan:Connect(ShiftPressed)
    UIS.InputEnded:Connect(ShiftReleased)
end)


MainSection:NewButton("(Bloxr-Run)(Lock4Fun-Style)", "(Fast but not really. Better for people who aimlock.)", function()
    local player = game:GetService("Players").LocalPlayer
    local UIS = game:GetService("UserInputService")
    
