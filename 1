local Main = Window:NewTab("Safe/TP")
local MainSection = Main:NewSection("Safe/TP")


MainSection:NewButton("(Infinite-Jump(Don-Style)", "(Fatboy-Inspiration.)", function()
 local InfiniteJumpEnabled = true
game:GetService("UserInputService").JumpRequest:connect(function()
	if InfiniteJumpEnabled then
		game:GetService"Players".LocalPlayer.Character:FindFirstChildOfClass'Humanoid':ChangeState("Jumping")
	end
end)
end)


MainSection:NewButton("(Set-Waypoint)", "(Will-Set-A-Waypoint-Where-You-Are-Currently-At.)", function()
    waypoint = game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.CFrame
end)


MainSection:NewButton("(Goto-Waypoint)", "(Will-TP-You-To-Your-Set-Waypoint.)", function()
    game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.CFrame = waypoint
end)


